{
    "name": "gupalo/auditlog-bundle",
    "description": "Audit Log Bundle",
    "type": "symfony-bundle",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Gupalo\\AuditLogBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.11|^3.0",
        "symfony/form": "^6.0|^7.0",
        "symfony/security-core": "^6.0|^7.0",
        "symfony/translation": "^6.0|^7.0",
        "symfony/yaml": "^6.0|^7.0",
        "symfony/event-dispatcher": "^6.0|^7.0",
        "symfony/twig-bundle": "^6.0|^7.0",
        "symfony/orm-pack": "^2.2",
        "symfony/asset": "^6.0|^7.0",
        "symfony/validator": "^6.0|^7.0",
        "sensio/framework-extra-bundle": "^6.2|^7.0",
        "gupalo/dateutils": "^1.13"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.2"
        },
        "sort-packages": true
    }
}
